    "User": {
        "allowed_resolutions": ["160x120", "300x200", "720x500"]
    },
    "MonitoringStatus": {

    },
    "JobQueueBundle": {
        "database_service_id": "db_main"
    },
    "SystemConfigBundle": {
        "database_service_id": "db_main"
    },
    "NoticeBundle": {
        "database_service_id": "db_main",
        "cache_path": "%NoticeBundle.cache_path%",
        "image_storage_path": "%NoticeBundle.image_storage_path%",
        "locales": "%locales%",
        "database_type": "relational"
    },
    "BackendBundle": {},
    "FaqBundle": {
        "database_type": "relational",
        "database_service_id": "db_main",
        "locales": "%locales%",
        "email_sender_name": "%EMAIL_SENDER_NAME%",
        "email_sender_address": "%EMAIL_SENDER_ADDRESS%"
    },
    "StaticPageBundle": {
        "database_type": "relational",
        "database_service_id": "db_main",
        "cache_path": "../cache/static-page/",
        "locales": "%locales%"
    },
    "AgencyBundle": {
        "database_service_id": "db_main",
        "aws_s3_service_id": "amazon_aws_s3",
        "aws_s3_bucket_public": "%AWS_S3_BUCKET_PUBLIC%",
        "image_cache_gaufrette_adapter_service_id": "agency_gaufrette_aws_s3_images_cache_adapter",
        "locales": "%locales%"
    }
